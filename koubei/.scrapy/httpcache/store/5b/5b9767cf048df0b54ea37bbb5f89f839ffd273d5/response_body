		var oTip = document.createElement('div');
		oTip.innerHTML = sHtml;
		YK.D.insertBefore(oTip,document.body.firstChild);
		
		var myAnim = new YAHOO.util.Anim('backcity-tip', {
			marginTop: {
					to: 0 
			} 
		}, 0.6, YAHOO.util.Easing.easeIn);
		var myAnim2 = new YAHOO.util.Anim('backcity-tip-c', {
				marginTop: {
						to: 0 
				} 
		}, 0.3);

		var myAnim3 = new YAHOO.util.Anim('backcity-b', {
