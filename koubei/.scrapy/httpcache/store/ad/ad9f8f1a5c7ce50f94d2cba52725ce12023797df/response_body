	if(typeof(YK.vars.cityName) != "undefined"&&YK.vars.city==2595&&YAHOO.util.Cookie.get('MyKoubeiCity')!=YK.vars.city&&YAHOO.util.Cookie.get('MyKoubeiCity')!=null){
