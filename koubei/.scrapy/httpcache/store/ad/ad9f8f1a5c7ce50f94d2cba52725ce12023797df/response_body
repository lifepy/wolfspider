<script charset='utf-8' src="http://k.kbcdn.com/global/utilities/utilities-1-4-43.js"></script>



<script>
//global vars
YK.set('YK.vars.city', YK.D.get('k2-h-wrap')&&YK.D.get('k2-h-wrap').getAttribute('data-cityid'));
YK.set('YK.vars.channel',YK.D.get('k2-h-wrap')&&YK.D.get('k2-h-wrap').getAttribute('data-channel'));
YK.set('YK.vars.cityName',YK.D.get('k2-h-wrap')&&YK.D.get('k2-h-wrap').getAttribute('data-cityname'));
YK.set('YK.vars.time',(function(){
	var str = YK.D.get('k2-h-wrap')&&YK.D.get('k2-h-wrap').getAttribute('data-time');
	var d = new Date();
	d.setTime(str);
	return d;
})());
//break wrod
YK.util.breakWord();
//fix ie6 background-image cache bug
if(YAHOO.env.ua.ie == 6){
	document.execCommand("BackgroundImageCache", false, true);		
}

//map button
(function(){
	var ipt = YK.D.get('k2-h-search-ipt');
	function randomNumForSearch(n){
	  var result="";
	  for(var i=0;i<n;i++)
		result+=Math.floor(Math.random()*10);
	  return result;
	}
	
	if(YK.D.inDocument('k2-h-btn-map')){
		YK.E.on(YK.D.get('k2-h-btn-map'), 'click', function(){

			var cache = randomNumForSearch(7);
			var cImg = document.createElement('img');
			cImg.src = 'http://www.atpanel.com/search?cache=' + cache + '&kind=' + 'kb.header.search.ditu';

			var keyword = ipt.value,
				charset;
			if(('huangye' === YK.vars.channel) || ('shanghu' === YK.vars.channel)){
				charset = 'gbk';
			}else{	
				charset = 'utf8';
			}
			location.href = 'http://ditu.koubei.com/mapsearch/mapsearch.html?ei=' + charset + '&keyword=' + keyword + '&city=' + YK.vars.city + '#inview=0';
		});
	}
	
	if(YK.D.inDocument('k2-h-btn-youhui')){
		function goYouhui(){
			var keyword = ipt.value;
			location.href = 'http://www.koubei.com/fuwu/list.html?frontCategoryId=944&keyword=' + encodeURI(keyword) + '&city=' + YK.vars.city;
		}
		YK.E.on(YK.D.get('k2-h-btn-youhui'), 'click', function(){
			goYouhui();
		});
		YK.E.on(YK.D.get('k2-h-search-ipt'), 'keyup', function(e){
			if(e.keyCode === 13){
				goYouhui();
			}
		});
	}
	
	if(YK.D.inDocument('k2-h-btn-order')){
		function goOrder(){
			var keyword = ipt.value;
			location.href = 'http://waimai.koubei.com/web/takeoutStoreList.html?orderBy=order_quantity&sort=DESC&keywordName=' + encodeURI(keyword) + '&city=' + YK.vars.city;
		}
		YK.E.on(YK.D.get('k2-h-btn-order'), 'click', function(){
			goOrder();
		});
		YK.E.on(YK.D.get('k2-h-search-ipt'), 'up', function(){
			if(e.keyCode === 13){
				goOrder();
			}
		});
	}
	
	if(YK.D.inDocument('k2-h-btn-youhui') || YK.D.inDocument('k2-h-btn-order')){
		YK.E.on(YK.D.get('k2-h-btn-bendi'), 'click', function(){
			var keyword = ipt.value;
			location.href = 'http://bendi.koubei.com/search/searchstore.html?ei=utf8&keyword=' + encodeURI(keyword) + '&city=' + YK.vars.city;
		});
	}
	
	
})();

//autocomplete
(function() {
if(YK.vars.channel === 'youhui' || YK.vars.channel === 'order') return;
	var Dom = YAHOO.util.Dom,Event = YAHOO.util.Event;
	if (Dom.inDocument('k2-h-aclist')&&YK.D.get('k2-h-search-form').action=='http://bendi.koubei.com/search/searchstore.html') {
		var oLoader = new YAHOO.util.YUILoader({
			require : ['connection','datasource'],
			onSuccess : function(){}
		});

		oLoader.addModule({
			'type': 'js',
			'name': 'ykautocomplete',
			'fullpath': 'http://k.kbcdn.com/global/autocomplete/autocomplete_1_0_1.js'
		});
		oLoader.addModule({
			'type': 'js',
			'name': 'header_autocomplete',
			'fullpath': 'http://k.kbcdn.com/product/common/header/header_autocomplete-1-2-12.js',
			'requires': ['ykautocomplete']
		});
		oLoader.require(["header_autocomplete"]);
		oLoader.insert({}, 'js');
	}
if (YK.vars.channel === 'hp' && YK.D.inDocument('k2-h-search-ipt')) {
		YK.D.get('k2-h-search-ipt').focus();
	}

