			var Link11="http://suzhou.koubei.com/store/detail--storeId-7a4b270dfe4641d789ef673db50d4633";
			nowtotalcount ++;
		
	
		
			
			var FullName12="大娘水饺(石路店)";
			var Posx12="12060590";
			var Posy12="3130601";
			var Link12="http://suzhou.koubei.com/store/detail--storeId-9370e7f1c6d845a4ab9d12e444dbe2ed";
			nowtotalcount ++;
		
	
	
	var markerlist=null;
	function getFunCallBack(PointNum){
		return function(){ChangeCurMarker(PointNum);};
	}
	
	function getFunClickCallBack(PointNum){
		return function(){
			document.getElementById("usemap").action=eval("Link"+PointNum);
			document.getElementById("usemap").submit();
		}
	}
	
	function ChangeCurMarker(thenum){
		try{
		maps.clearOverlays();
		}catch(e){}
		var curmarker=null;
		if (centermarker!=null){
			maps.addOverlay( centermarker );
		}
		
		
		
		for (i=1;i<=nowtotalcount;i++){
			tempposx=eval("Posx"+i);
			tempname=eval("FullName"+i);
			if (tempposx!="" && tempposx!="0"){
				var point = eval("new AliLatLng(Posy"+i+"/100000,Posx"+i+"/100000)");
				var marker;
				if (thenum!=i){
					marker = new AliPointOverlay(point,createMarkerHtml(i,false),{offset:{x:0, y:-30}});
					maps.addOverlay(marker);
					AliEvent.addListener(marker,"mouseover",getFunCallBack(i));
					AliEvent.addListener(marker,"click",getFunClickCallBack(i));					
				}else{
					marker = new AliPointOverlay(point,createMarkerHtml(i,true),{offset:{x:0, y:-30}});
					curmarker=marker;
					AliEvent.addListener(marker,"click",getFunClickCallBack(i));
				}
			}
		}
		
		try{
		maps.addOverlay(curmarker);
		}catch(e){}
	}
	
	function ChangeCurMarker2(thenum){
		var curmarker=null;
		for (i=0 ;i<markerlist.length;i++){
			marker=markerlist[i];
			if (marker==null) continue;
			try{
			AliEvent.clearListeners(marker,"mouseover");
			if (i+1==thenum){	
				marker.setContainer(createMarkerHtml(thenum,true));
				curmarker=marker;
			}else{
				marker.setContainer(createMarkerHtml(i+1,false));
			}
			}catch(e){;}
		}
		if (curmarker!=null){
		 	curmarker.focus();
		}
	}
	
	function createMarkerHtml(markerid,iscur){
		if (iscur){
			html = "<div class='list-map-icon'><div class='active-num yk-inline-block'>"+markerid+"</div>";	
			html += "<div class='active-content yk-inline-block'>"+eval("FullName"+markerid)+"</div>";
			html += "</div>"
		}else{
			html = "<div class='list-map-icon'><div class='defaul-content'>"+markerid+"</div></div>";
		}
		return html;
	}
	
	function ShowOnMap(){
		maps.clearOverlays();
		markerlist=new Array();
		var points=new Array();
		if (centermarker!=null){
			maps.addOverlay( centermarker );
		}
		for (i=1;i<=nowtotalcount;i++){
			tempposx=eval("Posx"+i);
			tempname=eval("FullName"+i);
			if (tempposx!="" && tempposx!="0"){
				var point = eval("new AliLatLng(Posy"+i+"/100000,Posx"+i+"/100000)");
				points.push(point);
				var marker = new AliPointOverlay(point,createMarkerHtml(i,false),{offset:{x:0, y:-30}});
				maps.addOverlay(marker);
				AliEvent.addListener(marker,"mouseover",getFunCallBack(i));
				AliEvent.addListener(marker,"click",getFunClickCallBack(i));
				markerlist.push(marker);
			}else{
				markerlist.push(null);
			}
		}
		var thebounds=AliBounds.getLatLngBounds(points);
		var thezoom=maps.getBoundsZoom(thebounds);
	    var boundpoints=new Array();
	    var thepoint;
	    var start=0;
	    var iterator=0;
	    while(true){
	    	 for (i=0;i<points.length;i++){
	            if (i!=start){
		            thepoint=points[i];
		            boundpoints.push(thepoint);
	            }
	         }
	         nowzoom=maps.getBoundsZoom(AliBounds.getLatLngBounds(boundpoints));
	         if (nowzoom>thezoom){
	            points.length=0;
	            for (j=0;j<boundpoints.length;j++){
	               points.push(boundpoints[j]);
	            }
	            thezoom=nowzoom;
	            iterator++;
	            start=0;
	            if (iterator>=2) break;
	            continue;
	         }
	         boundpoints.length=0;
	         if (start==points.length) break;
	         start++;
	    }
	    thebounds=AliBounds.getLatLngBounds(points);
	    thezoom=maps.getBoundsZoom(thebounds);
	    maps.centerAndZoom(thebounds.getCenter(),thezoom);
	}
	
	try{
		maps = new AliMap( document.getElementById( "map-body" ) );
		var centerp=new AliLatLng(centery/100000,centerx/100000);
		var icon=new AliIcon("http://k.kbcdn.com/product/huangye/cate_list/img/marker-blue.png",{x:22,y:22},{x:11,y:22});
		var iconHover=new AliIcon("http://k.kbcdn.com/product/huangye/cate_list/img/marker-blue-hover.png",{x:22,y:22},{x:11,y:22});
		
		
		
		
		
		
		
		
		centermarker=new AliMarker(centerp);
		centermarker.setIcon(icon);
		
		if(YK.D.inDocument('store-list-diwu')){
			var diwu = YK.D.get('store-list-diwu'),
				diwuMarker = YK.D.getFirstChild(diwu);
			YK.E.on(diwu, 'mouseover', function(){
				YK.D.setStyle(diwu, 'backgroundColor', '#FEFFD7');
				YK.D.replaceClass(diwuMarker, 'marker-diwu', 'marker-diwu-hover');
				centermarker.setIcon(iconHover);
			});
			YK.E.on(diwu, 'mouseout', function(){
				YK.D.setStyle(diwu, 'backgroundColor', '#fff');
				YK.D.replaceClass(diwuMarker, 'marker-diwu-hover', 'marker-diwu');
				centermarker.setIcon(icon);
			});
		}
		
		
		control=new AliMapLargeControl({hidePanBtn:true,hideZoomBtn:false,hideZoomBar:true});
		maps.addControl(control);
		maps.addOverlay( centermarker );
		//maps.addControl(new AliMapKeyboardControl()); 		//键盘控制
		maps.centerAndZoom(centerp,12);
		ShowOnMap();
		
		
		
		
		if (document.getElementById( "curcenterx" )!=null && document.getElementById( "curcentery" )!=null){
			document.getElementById( "curcenterx" ).value=centerx;
			document.getElementById( "curcentery" ).value=centery;
		}
	}catch(e){;}
	
	YK.E.on(YK.D.getElementsByClassName('store-item','LI','store-list'),'mouseover',function(ev){
	      if(this.getAttribute('num') != null){
	         ChangeCurMarker(this.getAttribute('num'));
	      }   
	   	});
	   	
	   	YK.E.on(YK.D.getElementsByClassName('store-item','LI','store-list'),'mouseout',function(ev){
	      if(this.getAttribute('num') != null){
	         ChangeCurMarker(-100);
	      }   
	   	});
	   	
</script>

