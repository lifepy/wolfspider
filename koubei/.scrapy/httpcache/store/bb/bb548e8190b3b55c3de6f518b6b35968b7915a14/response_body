(function(){
  if(YK.L.isNull(YAHOO.util.Cookie.get('MyKoubeiCity'))&&!YK.L.isNull(YK.vars.city)){
    var dExpires = new Date();
    dExpires.setDate(dExpires.getDate()+365);
    YAHOO.util.Cookie.set('MyKoubeiCity',YK.vars.city,{path:'/',domain:'.koubei.com',expires:dExpires })
  }
